nuint fib(nuint n) {
    nuint prev1 = 0;
    nuint prev2 = 1;
    
    nuint i = 2;
    while i <= n {
        if i == 14 {
            i += 1;
            continue;
        }
        nuint c = prev1 + prev2;
        prev1 = prev2;
        prev2 = c;
        i += 1;
    }

    return prev2;
}

